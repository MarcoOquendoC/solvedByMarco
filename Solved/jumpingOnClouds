function jumpingOnClouds(c) {
  //https://www.hackerrank.com/challenges/jumping-on-the-clouds/problem
    let steps=0
    for(let i=1;i<c.length;i++){
        if(c[i]==1 || (c[i]==0 && c[i+1]==0)){
            steps++;i++
            }else{
                steps++
            }//end else
    }//end for
    return steps
  //jumpingOnClouds([0,0,0,0,1,0,1,0,1,0])
}
